{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ammosdatafactory"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"InputDB_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'InputDB'"
		},
		"InputFilesCopy_DB_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'InputFilesCopy_DB'"
		},
		"Output_DB_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Output_DB'"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0205-044034-3ovu162z"
		},
		"InputDB_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ammosdatastorage.dfs.core.windows.net/"
		},
		"InputFilesCopy_DB_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ammosdatastorage.dfs.core.windows.net/"
		},
		"Output_DB_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ammosdatastorage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ammosdatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"InputDB_accountKey": {
						"value": "[parameters('InputDB_accountKey')]"
					},
					"InputFilesCopy_DB_accountKey": {
						"value": "[parameters('InputFilesCopy_DB_accountKey')]"
					},
					"Output_DB_accountKey": {
						"value": "[parameters('Output_DB_accountKey')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"InputDB_properties_typeProperties_url": {
						"value": "[parameters('InputDB_properties_typeProperties_url')]"
					},
					"InputFilesCopy_DB_properties_typeProperties_url": {
						"value": "[parameters('InputFilesCopy_DB_properties_typeProperties_url')]"
					},
					"Output_DB_properties_typeProperties_url": {
						"value": "[parameters('Output_DB_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}